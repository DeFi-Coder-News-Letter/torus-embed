{
  "name": "@toruslabs/torus-embed",
  "version": "0.2.11",
  "description": "Embed script for Torus",
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torusresearch/torus-embed.git"
  },
  "scripts": {
    "build:babel": "babel ./src --out-dir dist/ && cp ./src/embed.d.ts ./dist/embed.d.ts",
    "build": "rimraf ./dist && npm run build:babel && cross-env TORUS_BUILD_ENV=production npm run build-embed && npm run post-build",
    "build:staging": "rimraf ./dist && npm run build:babel && cross-env TORUS_BUILD_ENV=staging npm run build-embed && npm run post-build",
    "build:testing": "rimraf ./dist && npm run build:babel && cross-env TORUS_BUILD_ENV=testing npm run build-embed && npm run post-build",
    "build:dev": "rimraf ./dist && npm run build:babel && cross-env TORUS_BUILD_ENV=development npm run build-embed && npm run post-build",
    "build-embed": "node ./scripts/createEmbed.js",
    "lint:js": "eslint --fix src/**/*.js",
    "post-build": ". ./scripts/calculateIntegrity.sh",
    "test:e2e": "node ./test/e2e/puppeteer.js",
    "test:e2e:headful": "node ./test/e2e/puppeteer.js false",
    "test:build-embed": "mocha --exit ./test/compile/Web3Test.js",
    "update:gist": ". ./scripts/updateGistIntegrity.sh"
  },
  "dependencies": {
    "@toruslabs/fetch-node-details": "^1.0.0",
    "@toruslabs/torus.js": "^1.0.4",
    "json-rpc-engine": "^5.1.6",
    "json-rpc-middleware-stream": "^2.1.1",
    "loglevel": "^1.6.6",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^4.0.3",
    "post-message-stream": "^3.0.0",
    "pump": "^3.0.0",
    "random-id": "^1.0.3",
    "readable-stream": "^3.4.0",
    "sri-toolbox": "^0.2.0",
    "through2": "^3.0.1",
    "web3": "0.20.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "assert": "^2.0.0",
    "browserify": "^16.5.0",
    "cross-env": "^6.0.3",
    "discify": "^1.6.3",
    "envify": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "husky": "^4.0.5",
    "lint-staged": "^9.5.0",
    "mocha": "^7.0.0",
    "mocha-eslint": "^6.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "uglifyify": "^5.0.2"
  },
  "mocha": {
    "timeout": 0
  },
  "keywords": [
    "torus",
    "wallet",
    "ERC20",
    "Ethereum",
    "embed",
    "login",
    "OAuth",
    "crypto"
  ],
  "author": "Torus Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/torusresearch/torus-embed/issues"
  },
  "homepage": "https://github.com/torusresearch/torus-embed#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js --",
      "prettier --write **/*.js",
      "git add"
    ],
    "*.{json}": [
      "prettier --write **/*.json",
      "prettier --write **/*.css",
      "git add"
    ]
  }
}
